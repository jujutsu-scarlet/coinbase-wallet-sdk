# Copy this file to .env before running locally
# cp .env.example .env

# Ethereum network RPC URL (local or remote node)
RPC_URL=http://localhost:8545

# Network name (e.g., Ethereum mainnet, Rinkeby, or custom)
NETWORK=development

# Optional API key (if required by your environment)
API_KEY=

# Logging level (info | debug | warn)
LOG_LEVEL=info
